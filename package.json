    "@rollup/plugin-terser": "^0.3.0",
    "@surma/rollup-plugin-off-main-thread": "^2.2.3",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.11.18",
    "cssnano": "^5.0.17",
    "del": "^7.0.0",
    "glob": "^8.1.0",
    "image-size": "^1.0.1",
    "lodash.camelcase": "^4.3.0",
    "mime-types": "^2.1.34",
    "postcss": "^8.4.6",
    "postcss-modules": "^6.0.0",
    "postcss-nested": "^6.0.0",
    "postcss-url": "^10.1.3",
    "preact": "^10.6.5",
    "preact-render-to-string": "^5.1.19",
