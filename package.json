{
  "name": "wordle-analyzer",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c && node lib/move-output.js",
    "debug": "node --inspect-brk node_modules/.bin/rollup -c",
    "dev": "DEV_PORT=\"${DEV_PORT:=5000}\" rollup -cw & npm run serve",
    "serve": "serve --config server.json .tmp/build/static",
    "dockerbuild": "docker build -t wordle-analyzer_container .",
    "dockerrun": "cross-env-shell \"docker run --name wordle-analyzer -p 3000:3000 -v $INIT_CWD:/app wordle-analyzer_container\"",
    "dockerstart": "docker start wordle-analyzer"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@surma/rollup-plugin-off-main-thread": "^2.2.3",
    "@types/mime-types": "^2.1.1",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.17",
    "del": "^6.0.0",
    "glob": "^7.2.0",
    "image-size": "^1.0.1",
    "lodash.camelcase": "^4.3.0",
    "mime-types": "^2.1.34",
    "postcss": "^8.4.6",
    "postcss-modules": "^4.3.0",
    "postcss-nested": "^5.0.6",
    "postcss-url": "^10.1.3",
    "preact": "^10.6.5",
    "preact-render-to-string": "^5.1.19",
    "rollup": "^2.67.2",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^13.0.2",
    "typescript": "^4.5.5"
  }
}